{
  "version": 2,
  "builds": [
    {
      "src": "api/refresh-token.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/refresh-token",
      "dest": "api/refresh-token.ts"
    }
  ],
  "cron": [
    {
      "path": "/api/refresh-token",
      "schedule": "0 0 */15 * *"
    }
  ]
}
